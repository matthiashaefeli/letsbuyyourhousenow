<%= form_for(@client, html: { onsubmit: 'addClient(event, this)' }) do |f| %>
  <div class="container">
    <div>
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>
    </div>
    <div>
      <%= f.label :last_name %>
      <%= f.text_field :last_name %>
    </div>
    <div>
      <%= f.label :address %>
      <%= f.text_field :address %>
    </div>
    <div>
      <%= f.label :zip %>
      <%= f.text_field :zip %>
    </div>
    <div>
      <%= f.label :city %>
      <%= f.text_field :city %>
    </div>
    <div>
      <%= f.label :state %>
      <%= f.text_field :state %>
    </div>
    <div>
      <%= f.label :tel %>
      <%= f.text_field :tel %>
    </div>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <div>
      <%= f.submit 'Save', id: 'clientSubmit' %>
    </div>
  </div>
<% end %>
